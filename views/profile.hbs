<a href='/dashboard'>Go to dashboard</a>


<form method="post">

<input type="hidden"
          value="{{this.profId}}"
          name="profId"
          
        />
  {{#each fields}}
    <div>
      
      <label for="{{this.name}}">
        {{this.label}}
        <input
          id="{{this.name}}"
          {{#if this.type}}type="{{this.type}}"{{/if}}
          name="{{this.name}}"
          {{#if this.required}}required{{/if}}
          value="{{this.value}}"
        />
      </label>
    </div>
  {{/each}}

  <button type="submit">Save</button>
</form>
